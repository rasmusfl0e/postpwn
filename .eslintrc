{
	"env": {
		"browser": true,
		"commonjs": true
	},
	"rules": {
		"eqeqeq": [2, "smart"],
		"guard-for-in": 2,
		"no-caller": 2,
		"no-empty": 2,
		"no-undef": 2,
		"curly": 2,
		"strict": [2, "never"],
		"no-mixed-spaces-and-tabs": 2,
		"no-underscore-dangle": 0,
		"no-shadow": 0,
		"no-fallthrough": 0,
		"no-use-before-define": 0
	}
}